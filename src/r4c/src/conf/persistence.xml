<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="r4cPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>br.edu.utfpr.cp.r4c.model.Typeprofile</class>
    <class>br.edu.utfpr.cp.r4c.model.Question</class>
    <class>br.edu.utfpr.cp.r4c.model.Project</class>
    <class>br.edu.utfpr.cp.r4c.model.Statues</class>
    <class>br.edu.utfpr.cp.r4c.model.Task</class>
    <class>br.edu.utfpr.cp.r4c.model.Anwser</class>
    <class>br.edu.utfpr.cp.r4c.model.Questionary</class>
    <class>br.edu.utfpr.cp.r4c.model.Tagpattern</class>
    <class>br.edu.utfpr.cp.r4c.model.Profile</class>
    <exclude-unlisted-classes>false</exclude-unlisted-classes>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:postgresql://localhost:5432/r4c"/>
      <property name="javax.persistence.jdbc.user" value="postgres"/>
      <property name="javax.persistence.jdbc.driver" value="org.postgresql.Driver"/>
      <property name="javax.persistence.jdbc.password" value="postgres"/>
    </properties>
  </persistence-unit>
</persistence>
